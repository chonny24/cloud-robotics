<launch>
  <arg name="initial_pose_x" default="0.0"/> 
  <arg name="initial_pose_y" default="0.0"/> 
  <arg name="initial_pose_a" default="0.0"/>
  <arg name="scan_topic" default="base_scan"/>
  <arg name="amcl_node_name" default="client_pr2_amcl"/>
  <arg name="computation_loc" value="client"/>
  
  <include file="$(find cloud_robotics)/launch/cr_amcl/pr2_cr_amcl.launch.xml">
    <arg name="amcl_node_name" value="$(arg amcl_node_name)"/>
    <arg name="scan_topic" value="$(arg scan_topic)"/>
    <arg name="computation_loc" value="$(arg computation_loc)"/>
  </include>

</launch>
